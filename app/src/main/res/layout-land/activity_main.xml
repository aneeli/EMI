<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.aneeli.emi_calculator.MainActivity"
    android:background="@drawable/dollar_bg_converted"
    android:focusable="false"
    android:clickable="false">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/principal"
        android:id="@+id/textView"
        android:textStyle="bold"
        android:textSize="18sp"
        android:textIsSelectable="false"
        android:background="#ffe54f"
        android:textColor="#000000"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/editText"
        android:hint="@string/zero"
        android:background="#fff56d"
        android:textColor="#000000"
        android:layout_alignTop="@+id/textView"
        android:layout_alignLeft="@+id/editText2"
        android:layout_alignStart="@+id/editText2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/interest"
        android:id="@+id/textView2"
        android:textSize="18sp"
        android:textStyle="bold"
        android:background="#ffe54f"
        android:textColor="#000000"
        android:layout_below="@+id/textView"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/editText2"
        android:hint="@string/zero_interest"
        android:background="#fff56d"
        android:textColor="#000000"
        android:layout_above="@+id/frameLayout"
        android:layout_toRightOf="@+id/textView"
        android:layout_toEndOf="@+id/textView"
        android:layout_marginLeft="40dp"
        android:layout_marginStart="40dp" />

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#ffe54f"
        android:layout_below="@+id/textView9"
        android:layout_alignRight="@+id/textView17"
        android:layout_alignEnd="@+id/textView17"
        android:layout_alignBottom="@+id/textView15"
        android:layout_alignLeft="@+id/textView3"
        android:layout_alignStart="@+id/textView3"
        android:id="@+id/frameLayout"></FrameLayout>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/five"
        android:id="@+id/textView3"
        android:textIsSelectable="true"
        android:textSize="20sp"
        android:textStyle="bold"
        android:background="#ff9f32"
        android:layout_below="@+id/textView17"
        android:layout_alignLeft="@+id/textView5"
        android:layout_alignStart="@+id/textView5"
        android:layout_alignRight="@+id/textView5"
        android:layout_alignEnd="@+id/textView5"
        android:textColor="#000000" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/zero"
        android:id="@+id/textView4"
        android:layout_alignTop="@+id/textView3"
        android:layout_toRightOf="@+id/textView8"
        android:layout_toEndOf="@+id/textView8"
        android:textSize="20sp"
        android:textStyle="bold"
        android:background="#ff9f32"
        android:textColor="#000000" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/ten"
        android:id="@+id/textView5"
        android:textStyle="bold"
        android:textSize="20sp"
        android:background="#ff9395"
        android:layout_below="@+id/textView3"
        android:layout_toLeftOf="@+id/editText2"
        android:layout_toStartOf="@+id/editText2"
        android:textColor="#000000" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/zero"
        android:id="@+id/textView6"
        android:textStyle="bold"
        android:textSize="20sp"
        android:background="#ff9395"
        android:layout_alignBottom="@+id/textView5"
        android:layout_toRightOf="@+id/textView8"
        android:layout_toEndOf="@+id/textView8"
        android:textColor="#000000" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/fifteen"
        android:id="@+id/textView7"
        android:textStyle="bold"
        android:textSize="20sp"
        android:background="#60fbcd"
        android:layout_below="@+id/textView5"
        android:layout_alignLeft="@+id/textView5"
        android:layout_alignStart="@+id/textView5"
        android:textColor="#000000" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/$amount"
        android:id="@+id/textView8"
        android:background="#ff8031"
        android:textColor="#000000"
        android:textStyle="bold"
        android:layout_alignTop="@+id/editText"
        android:layout_alignLeft="@+id/textView9"
        android:layout_alignStart="@+id/textView9" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/$rate"
        android:id="@+id/textView9"
        android:background="#ff8031"
        android:textStyle="bold"
        android:textColor="#000000"
        android:layout_below="@+id/editText"
        android:layout_alignRight="@+id/frameLayout"
        android:layout_alignEnd="@+id/frameLayout"
        android:layout_marginRight="44dp"
        android:layout_marginEnd="44dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/zero"
        android:id="@+id/textView10"
        android:textSize="20sp"
        android:textStyle="bold"
        android:background="#60fbcd"
        android:layout_alignBottom="@+id/textView7"
        android:layout_alignLeft="@+id/textView6"
        android:layout_alignStart="@+id/textView6"
        android:textColor="#000000" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/twenty"
        android:id="@+id/textView11"
        android:textStyle="bold"
        android:textSize="20sp"
        android:background="#84cdfe"
        android:layout_below="@+id/textView7"
        android:layout_alignLeft="@+id/textView7"
        android:layout_alignStart="@+id/textView7"
        android:textColor="#000000" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/zero"
        android:id="@+id/textView12"
        android:textStyle="bold"
        android:textSize="20sp"
        android:background="#84cdfe"
        android:layout_alignBottom="@+id/textView11"
        android:layout_toRightOf="@+id/textView8"
        android:layout_toEndOf="@+id/textView8"
        android:textColor="#000000" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/twentyfive"
        android:id="@+id/textView13"
        android:textStyle="bold"
        android:textSize="20sp"
        android:background="#33fb89"
        android:layout_below="@+id/textView11"
        android:layout_alignLeft="@+id/textView11"
        android:layout_alignStart="@+id/textView11"
        android:textColor="#000000" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/zero"
        android:id="@+id/textView14"
        android:textSize="20sp"
        android:textStyle="bold"
        android:background="#33fb89"
        android:layout_alignBottom="@+id/textView13"
        android:layout_toRightOf="@+id/textView8"
        android:layout_toEndOf="@+id/textView8"
        android:textColor="#000000" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/thirty"
        android:id="@+id/textView15"
        android:textStyle="bold"
        android:textSize="20sp"
        android:background="#d7a4f4"
        android:layout_below="@+id/textView13"
        android:layout_alignLeft="@+id/textView13"
        android:layout_alignStart="@+id/textView13"
        android:textColor="#000000" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/zero"
        android:id="@+id/textView16"
        android:textSize="20sp"
        android:textStyle="bold"
        android:background="#d7a4f4"
        android:layout_alignBottom="@+id/textView15"
        android:layout_toRightOf="@+id/textView8"
        android:layout_toEndOf="@+id/textView8"
        android:textColor="#000000" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/month"
        android:id="@+id/textView17"
        android:textStyle="bold"
        android:textSize="20sp"
        android:background="#ffe54f"
        android:layout_below="@+id/textView9"
        android:textColor="#000000"
        android:layout_alignLeft="@+id/textView3"
        android:layout_alignStart="@+id/textView3" />

</RelativeLayout>
